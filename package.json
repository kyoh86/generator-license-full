{
  "name": "generator-license-full",
  "version": "1.0.1",
  "description": "A Yeoman generator to create license file.",
  "homepage": "https://github.com/kyoh86/generator-license-full#readme",
  "author": "kyoh86",
  "files": ["generators"],
  "main": "generators/app/index.js",
  "keywords": [
    "yeoman",
    "generator",
    "scaffold",
    "license",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^2.13.1",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^22.0.6"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1 && jest",
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyoh86/generator-license-full.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kyoh86/generator-license-full/issues"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "fuse.js": "^3.2.0",
    "generator-generator": "^4.0.2",
    "inquirer-autocomplete-prompt": "^0.12.2",
    "spdx-license-list": "^4.0.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  }
}
