{
  "name": "generator-license-full",
  "version": "1.0.2",
  "description": "A Yeoman generator to create license file.",
  "homepage": "https://github.com/kyoh86/generator-license-full#readme",
  "author": "kyoh86",
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "yeoman",
    "generator",
    "scaffold",
    "license",
    "yeoman-generator"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1 && jest",
    "pretest": "eslint .",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyoh86/generator-license-full.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kyoh86/generator-license-full/issues"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "dependencies": {
    "fuse.js": "^5.1.0",
    "generator-generator": "^4.0.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "merge": "^2.1.1",
    "spdx-license-list": "^6.1.0",
    "yeoman-generator": "^4.0.1",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  }
}
